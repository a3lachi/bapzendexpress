{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    }
  ],
  "static": {
    "glob": "public/**/*",
    "rewrites": {
      "source": "/(.*)",
      "destination": "/$1"
    }
  }
}
